{
    "$schema": "https://typedoc.org/schema.json",
    "entryPoints": [
        "src/index.ts"
    ],
    "out": "docs",
    "name": "QBZ-Downloader API Documentation",
    "readme": "README.md",
    "includeVersion": true,
    "excludePrivate": true,
    "excludeProtected": true,
    "excludeExternals": true,
    "theme": "default",
    "navigation": {
        "includeCategories": true,
        "includeGroups": true
    },
    "categorizeByGroup": true,
    "searchInComments": true,
    "plugin": [],
    "exclude": [
        "**/*.test.ts",
        "**/*.spec.ts",
        "**/node_modules/**"
    ],
    "tsconfig": "./tsconfig.json"
}