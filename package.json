    "@types/express-rate-limit": "^5.1.3",
    "@types/figlet": "^1.7.0",
    "@types/gradient-string": "^1.1.6",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^22.0.0",
    "@types/node-cron": "^3.0.11",
    "@types/react": "^19.2.8",
    "@types/redis": "^4.0.10",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.56.0",
    "husky": "^9.1.7",
    "prettier": "^3.1.0",
    "typedoc": "^0.27.9",
    "typescript": "~5.7.3",
