{
  "name": "qbz-downloader",
  "version": "2.0.0",
  "description": "ðŸŽµ Premium Qobuz Downloader CLI with Hi-Res Audio, Complete Metadata & Embedded Lyrics",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "bin": {
    "qbz-dl": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    ".env.example"
  ],
  "scripts": {
    "build": "tsc && npm run copy-assets",
    "copy-assets": "copyfiles -u 1 \"src/services/dashboard/public/**/*\" dist",
    "start": "node dist/index.js",
    "dev": "tsc --watch",
    "dl": "node dist/index.js download",
    "bot": "node dist/index.js bot",
    "search": "node dist/index.js search",
    "test": "npx vitest run",
    "test:watch": "npx vitest",
    "test:coverage": "npx vitest run --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "docs": "typedoc --out docs src/index.ts",
    "prepare": "husky",
    "prepublishOnly": "npm run build && npm run test",
    "docker:build": "docker build -t qbz-downloader .",
    "docker:run": "docker run -it --env-file .env -v ./downloads:/app/downloads qbz-downloader"
  },
  "keywords": [
    "qobuz",
    "downloader",
    "hi-res",
    "flac",
    "music",
    "cli",
    "metadata",
    "lyrics",
    "lossless",
    "audio",
    "telegram-bot",
    "music-downloader",
    "hifi"
  ],
  "author": "ifauzeee",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ifauzeee/QBZ-Downloader.git"
  },
  "bugs": {
    "url": "https://github.com/ifauzeee/QBZ-Downloader/issues"
  },
  "homepage": "https://github.com/ifauzeee/QBZ-Downloader#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@types/archiver": "^7.0.0",
    "archiver": "^7.0.1",
    "axios": "^1.6.0",
    "boxen": "^7.1.1",
    "chalk": "^5.3.0",
    "cheerio": "^1.1.2",
    "cli-progress": "^3.12.0",
    "cli-table3": "^0.6.3",
    "commander": "^12.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "figlet": "^1.7.0",
    "flac-metadata": "^0.1.1",
    "gradient-string": "^2.0.2",
    "ink": "^6.6.0",
    "ink-big-text": "^2.0.0",
    "ink-gradient": "^3.0.0",
    "ink-multi-select": "^2.0.0",
    "ink-spinner": "^5.0.0",
    "inquirer": "^9.2.12",
    "node-cron": "^4.2.1",
    "node-id3": "^0.2.6",
    "ora": "^8.0.1",
    "p-limit": "^7.2.0",
    "react": "^19.2.3",
    "redis": "^5.10.0",
    "socket.io": "^4.8.3",
    "telegraf": "^4.16.3"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
